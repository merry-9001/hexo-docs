{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "sync": "set YUQUE_TOKEN=7tSew8wNmbWSsVwp2ltE3z7n7V1unklVFCqNupBn && yuque-hexo sync",
    "clean:yuque": "yuque-hexo clean",
    "all":  "yuque-hexo clean && set YUQUE_TOKEN=7tSew8wNmbWSsVwp2ltE3z7n7V1unklVFCqNupBn && yuque-hexo sync && hexo generate &&  && hexo deploy",
    "put":  "hexo generate && hexo deploy",
    "i":  "npm i -verbose",
    "hh":  "rm -rf /.deploy_git/*"
  },
  "hexo": {
    "version": "6.0.0"
  },
  "yuqueConfig": {
    "postPath": "source/_posts",
    "cachePath": "yuque.json",
    "mdNameFormat": "title",
    "adapter": "hexo",
    "concurrency": 5,
    "baseUrl": "https://www.yuque.com/api/v2",
    "login": "",
    "repo": "",
    "onlyPublished": true,
    "onlyPublic": true
  },
  "dependencies": {
    "hexo": "^6.0.0",
    "hexo-deployer-git": "^3.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^4.0.0",
    "hexo-server": "^2.0.0",
    "hexo-theme-landscape": "^0.0.3",
    "node-sass": "^7.0.1",
    "yuque-hexo": "^1.7.0",
    "hexo-renderer-stylus": "*"
  }
}
